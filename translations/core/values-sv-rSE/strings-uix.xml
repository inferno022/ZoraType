  <!-- Payment screen -->
  <string name="payment_screen_short_title">Betalningar</string>
  <string name="payment_screen_title">Betala för FUTO Tangentbord</string>
  <string name="payment_screen_sales_paragraph_1">&lt;string name="payment_screen_sales_paragraph_1"&gt;Du har använt FUTO Tangentbord i &lt;xliff:g name="days" example="30"&gt;%d&lt;/xliff:g&gt; dagar. Om du tycker att appen är användbar får du gärna stödja fortsatt utveckling av FUTO:s mjukvara.&lt;/string&gt;</string>
  <string name="payment_screen_sales_paragraph_1_alt">Tack för att du provar FUTO Tangentbord! Om du tycker att appen är användbar får du gärna stödja fortsatt utveckling av FUTO:s mjukvara.</string>
  <string name="payment_screen_sales_paragraph_2">FUTO bygger tillförlitlig mjukvara som inte utnyttjar dig. Den här appen kommer aldrig att visa annonser eller samla in din personliga data.</string>
  <string name="payment_screen_sales_point_development_title">Hållbar Utveckling</string>
  <string name="payment_screen_sales_point_development_body">FUTO:s uppdrag är att få öppen källkod-program och icke-skadliga affärsmetoder att bli hållbara inkomstkällor för projekt och deras utvecklare. Av den här anledningen föredrar vi att användare betalar för mjukvara.</string>
  <string name="payment_screen_sales_point_privacy_title">Åtagande för Integritet</string>
  <string name="payment_screen_sales_point_privacy_body">Den här appen kommer aldrig att visa annonser eller sälja din data. Vi sysslar inte med sånt.</string>
  <string name="payment_screen_error_build_unsupported_payment">Betalningar stöds inte i det här bygget</string>
  <string name="payment_screen_already_paid_title">Redan betalat?</string>
  <string name="payment_screen_already_paid_body">Om du redan har betalar för FUTO Tangentbord eller FUTO Röstinmatning, klicka nedan.</string>
  <string name="payment_screen_already_paid_button">Jag har redan betalat</string>
  <string name="payment_screen_already_paid_confirmation">Bekräfta ditt val</string>
  <string name="payment_screen_already_paid_confirmation_body">Är du säker på att du vill bekräfta att du redan har betalat? När det är bekräftat kommer du inte bli tillfrågad om betalning igen.</string>
  <string name="payment_screen_already_paid_confirmation_confirm">Jag har redan betalat</string>
  <string name="payment_screen_already_paid_confirmation_cancel">Avbryt</string>
  <string name="payment_screen_set_reminder_title">Inte redo att betala än?</string>
  <string name="payment_screen_set_reminder_body">Det här kommer att dölja påminnelsen i inställningarna under den valda tiden.</string>
  <string name="payment_screen_set_reminder_setter">När vill du bli påmind?</string>

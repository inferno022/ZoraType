  <string name="payment_screen_need_help_subtitle">Behöver du hjälp? Besök vår webbplats</string>
  <string name="payment_screen_pay_via_google">&lt;string name="payment_screen_pay_via_google"&gt;Betala via Google Play (&lt;xliff:g name="price" example="~$2.99"&gt;%s&lt;/xliff:g&gt; + skatt)&lt;/string&gt;</string>
  <string name="payment_screen_pay_via_futopay">&lt;string name="payment_screen_pay_via_futopay"&gt;Betala (&lt;xliff:g name="price" example="~$2.99"&gt;%s&lt;/xliff:g&gt; + skatt)&lt;/string&gt;</string>

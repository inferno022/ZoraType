  <string name="payment_screen_need_help_subtitle">Potřebujete poradit? Navštivte naše webové stránky</string>
  <string name="payment_screen_pay_via_google">&lt;string name="payment_screen_pay_via_google"&gt;Zaplatit skrze Google Play (&lt;xliff:g name="price" example="~$2.99"&gt;%s&lt;/xliff:g&gt; + daň)&lt;/string&gt;</string>
  <string name="payment_screen_pay_via_futopay">&lt;string name="payment_screen_pay_via_futopay"&gt;Zaplatit (&lt;xliff:g name="price" example="~$2.99"&gt;%s&lt;/xliff:g&gt; + daň)&lt;/string&gt;</string>
